{
  "name": "@k03mad/ip2geo",
  "version": "7.1.0",
  "description": "GeoIP library",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "bin": {
    "ip2geo": "app/cli.js"
  },
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k03mad/ip2geo.git"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@k03mad/request": "5.6.4",
    "@k03mad/simple-log": "2.1.3",
    "chalk": "5.3.0",
    "debug": "4.3.4"
  },
  "devDependencies": {
    "@k03mad/eslint-config": "20.7.0",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "mocha": "10.4.0"
  },
  "scripts": {
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./ --cache",
    "test": "rm -rfv ./.geoip && mocha tests",
    "clean": "npm run clean:modules && npm run clean:eslint:cache",
    "clean:modules": "rm -rf ./node_modules || true",
    "clean:eslint:cache": "rm -rf .eslintcache || true",
    "setup": "npm run clean && npm run setup:pnpm",
    "setup:pnpm": "npm i pnpm -g && pnpm i",
    "prepare": "husky || true"
  }
}
